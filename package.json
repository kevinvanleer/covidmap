    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "d3": "^5.16.0",
    "gh-pages": "^3.1.0",
    "kvl-react-ui": "^0.3.2",
    "lodash": "^4.17.20",
    "mapbox-gl": "^1.11.1",
    "moment": "^2.29.4",
    "rc-slider": "^9.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-gtm-module": "^2.0.11",
